.section-form{
    display: flex;
    flex-direction: column;
    width: 100%;
}
.section-form-item {
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 80rpx;
    margin-top: 40rpx;
}
.section-form-key {
    width: 260rpx;
    font-size: 16px;
    color: #666;
}
.section-form-value {
    flex-grow: 1;
    font-size: 20px;
}